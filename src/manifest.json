{
	"name": "GitHub Pull Request Tree",
	"short_name": "GitHub PRs",
	"permissions": [
		"*://*.github.com/*",
		"storage"
	],
	"content_scripts": [
		{
			"matches": [
				"*://*.github.com/*"
			],
			"run_at": "document_start",
			"js": [
				"main.js"
			]
		}
	],
	"icons": {
		"16": "16x16.png",
		"48": "48x48.png",
		"128": "128x128.png"
	},
	"manifest_version": 2,
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"options_ui": {
		"page": "options.html",
		"open_in_tab": false
	},
	"web_accessible_resources": [
		"*.png",
		"*.eot",
		"*.woff",
		"*.woff2",
		"*.ttf",
		"*.svg"
	]
}
